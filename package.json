{
  "name": "@tadamspace/conf-cli",
  "description": "[ts] simple cli for configuration and key/value's",
  "version": "0.1.9",
  "author": "Nathaniel Camomot, codeiqne",
  "bin": {
    "conf": "./bin/run"
  },
  "bugs": "https://github.com/codeiqne/conf-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.2",
    "@oclif/config": "^1.8.6",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^2.1.2",
    "@types/node": "^20.10.3",
    "conf": "^4.1.0",
    "tslib": "^1.9.3",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@oclif/test": "^3.1.3",
    "@types/mocha": "^10.0.6",
    "eslint": "^8.55.0",
    "eslint-config-oclif": "^5.0.0",
    "eslint-config-oclif-typescript": "^3.0.24",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10.2.0",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslint": "^5.11.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/codeiqne/conf-cli",
  "keywords": [
    "oclif",
    "conf",
    "cli",
    "config"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "conf",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "codeiqne/conf-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  }
}
